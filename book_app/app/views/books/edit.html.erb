<h1>Editing Book</h1>
<%= render 'form', book: @book %>
<%= form_with model:@book, url:"/books/#{@book.id}", local:true do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Body</h4>
  <%= f.text_area :body %>
  <%= f.submit 'Update Books' %>
<% end %>

<%= link_to 'Show', @book %> |
<%= link_to 'Back', books_path %>